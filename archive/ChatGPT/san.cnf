[ req ]
default_bits       = 2048
prompt             = no
default_md         = sha256
distinguished_name = dn
req_extensions     = req_ext
x509_extensions    = v3_ext
[ dn ]
C  = US
ST = California
L  = MyCity
O  = MyOrg
OU = MyUnit
CN = myapp.local
[ req_ext ]
subjectAltName = @alt_names
[ v3_ext ]
subjectAltName = @alt_names
[ alt_names ]
DNS.1 = myapp.local
DNS.2 = www.myapp.local
IP.1  = 127.0.0.1
